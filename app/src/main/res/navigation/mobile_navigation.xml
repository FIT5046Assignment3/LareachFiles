<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_home_fragment">

<fragment
        android:id="@+id/nav_add_fragment"
        android:name="com.fit5046.navigationjava.fragment.AddFragment"
        android:label="AddFragment" />
    <fragment
        android:id="@+id/nav_home_fragment"
        android:name="com.fit5046.navigationjava.fragment.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/nav_view_fragment"
        android:name="com.fit5046.navigationjava.fragment.ViewFragment"
        android:label="ViewFragment" />
    <fragment
        android:id="@+id/nav_quizEndResultModel"
        android:name="com.fit5046.navigationjava.fragment.QuizEndResultModel"
        android:label="QuizEndResultModel" />
    <fragment
        android:id="@+id/nav_quizMcqModel"
        android:name="com.fit5046.navigationjava.fragment.QuizMcqModel"
        android:label="QuizMcqModel" >
        <action
            android:id="@+id/action_nav_quizMcqModel_to_nav_quizWrittenModel"
            app:destination="@id/nav_quizWrittenModel" />
        <action
            android:id="@+id/action_nav_quizMcqModel_to_nav_quizEndResultModel"
            app:destination="@id/nav_quizEndResultModel" />
    </fragment>
    <fragment
        android:id="@+id/nav_quizWrittenModel"
        android:name="com.fit5046.navigationjava.fragment.QuizWrittenModel"
        android:label="QuizWrittenModel" >
        <action
            android:id="@+id/action_nav_quizWrittenModel_to_nav_quizMcqModel"
            app:destination="@id/nav_quizMcqModel" />
        <action
            android:id="@+id/action_nav_quizWrittenModel_to_nav_quizEndResultModel"
            app:destination="@id/nav_quizEndResultModel" />
    </fragment>
    <fragment
        android:id="@+id/nav_quizWelcome_fragment"
        android:name="com.fit5046.navigationjava.fragment.QuizWelcomeFragment"
        android:label="QuizWelcomeFragment" >
        <action
            android:id="@+id/action_nav_quizWelcome_fragment_to_nav_quizWrittenModel"
            app:destination="@id/nav_quizWrittenModel" />
        <action
            android:id="@+id/action_nav_quizWelcome_fragment_to_nav_quizMcqModel"
            app:destination="@id/nav_quizMcqModel" />
        <action
            android:id="@+id/action_nav_quizWelcome_fragment_self"
            app:destination="@id/nav_quizWelcome_fragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_testingabc_fragment"
        android:name="com.fit5046.navigationjava.fragment.TestingabcFragment"
        android:label="TestingabcFragment" >
        <action
            android:id="@+id/action_nav_testingabc_fragment_to_nav_quizWelcome_fragment"
            app:destination="@id/nav_quizWelcome_fragment" />
    </fragment>
</navigation>